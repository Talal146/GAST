<h2>User Details</h2>
<table clas="table-style">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach( User => { %>
      <tr>
      <td><%= User.name %></td>
      <td><%= User.email %></td>
      <% }); %>

  </tbody>
</table>

