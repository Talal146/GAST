<h2>Student Report</h2>
<table clas="table-style">
  <thead>
    <tr>
      <th>Name</th>
      <th>Grade (%)</th>
      <th>Grade Details</th>
      <th>Days of attendance</th>
      <th>Modification</th>

    </tr>
  </thead>
  <tbody>
    <% students.forEach( Student => { %>
      <tr>
      <td><%= Student.name %></td>
      <td><%= Student.total %></td>
      <td><a href="/homeworks/<%= Student._id %>">Grade Report</a></td>
      <td><%= Student.attendanceObject.status %></td>
      <td><a href="/students">Details</a></td>
      <% }); %>
      
      <% homework.gradeWaight%>

    </tr>
  </tbody>
</table>



