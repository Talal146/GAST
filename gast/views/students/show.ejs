<h2>Instructor Details</h2>
<table id="list">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone-Number</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach( User => { %>
      <tr>
      <td><%= User.name %></td>
      <td><%= User.email %></td>
      <td><%= User.phone %></td>
      <td><%= User.details ? 'Yes' : 'No' %></td>
      <% }); %>

  </tbody>
</table>

<%- include('../partials/footer') %>
<h2>Student Report</h2>
<table id="list">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Availabe Details</th>
      <th>Grade (%)</th>
      <th>Grade Details</th>
      <th>Attendance (%)</th>

    </tr>
  </thead>
  <tbody>
    <% students.forEach( Student => { %>
      <tr>
      <td><%= Student.name %></td>
      <td><%= Student.email %></td>
      <td><%= Student.phone %></td>
      <td><%= Student.details ? 'Yes' : 'No' %></td>
      <td><%= Student.total %></td>
      <td><a href="/homeworks/<%= Student._id %>">Grade Report</a></td>
      <td><%= Student.attendance %></td>
      </tr>
      <% }); %>

  </tbody>
</table>

<%- include('../partials/footer') %>