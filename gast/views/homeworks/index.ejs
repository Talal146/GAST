<%- include('../partials/header') %>
<h2>homeworks</h2>
<table id="list">
	<thead>
		<tr>
			<th>homework name</th>
			<th>deliverTime</th>
			<th>gradeWaight</th>
			<th>description</th>
			<th>Total</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<% homeworks.forEach((homework) => { %>
		</tr>

		<tr>
			<td><%= homework.name %></td>
			<td><%= homework.deliverTime %></td>
			<td><%= homework.gradeWaight %></td>
			<td><%= homework.description %></td>
      
			<td><% }); %></td>

			<td><%= homeworks.total %></td>
		</tr>
	</tbody>
</table>
<%- include('../partials/footer') %>
